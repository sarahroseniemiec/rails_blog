<h1>New Post</h1>
<div class="newpostform">
  <%= form_for @post do |form| %>
  <div class="">
    <%= form.label :title, "Title" %>
    <%= form.text_field :title %>
  </div>
  <div class="">
    <%= form.label :content, "Content" %>
    <%= form.text_area :content %>
  </div>
  <%= form.submit "enter" %>
  <% end %>
</div>
